#include "main.h"

/**
 *  *_memset - function that fills memory with constant bytes
 * @s: character parameter
 * @b: parameter 2
 * @n: parameter 3
 * Return: 0
 */
char *_memset(char *s, char b, unsigned int n)
{
	char *ptr = s;
	unsigned int i;

	for (i = 0; i < n; i++)
	{
		ptr[i] = (char)b;
	}
	return (s);
}
